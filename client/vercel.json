{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://esencialmentepsicologia.onrender.com/api/$1" }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
