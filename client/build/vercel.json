{
  "outputDirectory": "build",
  "buildCommand": "",
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://esencialmentepsicologia.onrender.com/sitemap.xml"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://esencialmentepsicologia.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}